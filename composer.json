{
    "name": "mdevoogd/laravel-db-enum-generator",
    "type": "library",
    "description": "Database enum generator for Laravel.",
    "keywords": [
        "database",
        "model",
        "eloquent",
        "generator",
        "laravel",
        "enum"
    ],
    "homepage": "https://github.com/mdevoogd/laravel-db-enum-generator",
    "license": "MIT",
    "authors": [{
        "name": "Mark de Voogd",
        "homepage": "https://github.com/mdevoogd",
        "role": "Developer"
    }],
    "require": {
        "php": ">=8.0",
        "illuminate/console": "^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "doctrine/inflector": "^2.0",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0",
        "phpunit/phpunit": ">=7.0",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "mdevoogd\\LaravelDatabaseEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mdevoogd\\LaravelDatabaseEnum\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
              "mdevoogd\\LaravelDatabaseEnum\\Providers\\LaravelDatabaseEnumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
